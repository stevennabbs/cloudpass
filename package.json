{
  "name": "cloudpass",
  "version": "1.3.0",
  "private": true,
  "description": "User management system",
  "keywords": [
    "user management"
  ],
  "author": {
    "email": "msuret@dhatim.com",
    "name": "Maxime Suret"
  },
  "homepage": "https://github.com/dhatim/cloudpass/issues",
  "bugs": {
    "url": "https://github.com/dhatim/cloudpass/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhatim/cloudpass.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=4.3.2",
    "npm": ">=2.13.1"
  },
  "main": "src/main.js",
  "dependencies": {
    "auth-header": "0.3.1",
    "bcryptjs": "2.3.0",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "config": "1.24.0",
    "continuation-local-storage": "3.2.0",
    "cookie-parser": "1.4.3",
    "es6-error": "4.0.0",
    "express": "4.14.0",
    "js-yaml": "3.7.0",
    "jsonwebtoken": "7.1.9",
    "lodash": "4.17.2",
    "moment": "2.17.0",
    "morgan": "1.7.0",
    "nodemailer": "2.6.4",
    "optional-js": "1.2.0",
    "passport": "0.3.2",
    "passport-http": "0.3.0",
    "passport-jwt-cookiecombo": "1.0.5",
    "pem": "1.9.2",
    "pg": "6.1.0",
    "pg-hstore": "2.3.2",
    "randomstring": "1.1.5",
    "saml2-js": "1.10.0",
    "sequelize": "3.27.0",
    "shimmer": "1.1.0",
    "ssacl": "0.1.3",
    "swagger-express-mw": "0.7.0",
    "throw": "1.1.0",
    "umzug": "1.11.0",
    "winston": "2.3.0",
    "winston-config": "0.5.1"
  },
  "devDependencies": {
    "bootprint": "0.8.5",
    "bootprint-openapi": "0.17.0",
    "coveralls": "2.11.15",
    "cross-conf-env": "1.0.7",
    "dockerode": "2.3.1",
    "dockerode-promise": "0.1.0",
    "env-test": "1.0.0",
    "gh-pages-travis": "1.0.0",
    "gulp": "3.9.1",
    "gulp-deb": "0.2.1",
    "gulp-rename": "1.2.2",
    "istanbul": "0.4.5",
    "jshint": "2.9.4",
    "merge-stream": "1.0.1",
    "mocha": "3.2.0",
    "rewire": "2.5.2",
    "rimraf": "2.5.4",
    "smtp-tester": "0.6.2",
    "sqlite3": "3.1.8",
    "superagent": "3.1.0",
    "supertest": "2.0.1",
    "supertest-as-promised": "4.0.2",
    "tar-fs": "1.14.0",
    "thenify": "3.2.1"
  },
  "scripts": {
    "start": "node --harmony .",
    "clean": "rimraf build",
    "test:unit": "cross-conf-env node --harmony node_modules/istanbul/lib/cli.js cover --config=tests/unit/.istanbul.yml node_modules/mocha/bin/_mocha -- --opts tests/mocha.opts  --reporter $npm_package_config_mocha_reporter tests/unit",
    "test:integration": "cross-conf-env node --harmony node_modules/istanbul/lib/cli.js cover --config=tests/integration/.istanbul.yml node_modules/mocha/bin/_mocha -- --opts tests/mocha.opts  --reporter $npm_package_config_mocha_reporter tests/integration",
    "lint": "jshint src",
    "pretest": "npm run lint",
    "test": "cross-conf-env node --harmony node_modules/istanbul/lib/cli.js cover --config=tests/.istanbul.yml node_modules/mocha/bin/_mocha -- --opts tests/mocha.opts --reporter $npm_package_config_mocha_reporter tests",
    "coveralls": "cat build/reports/coverage/global/lcov.info | coveralls",
    "api-doc": "bootprint openapi swagger/swagger.yaml build/reports/api-doc",
    "gh-pages": "gh-pages-travis",
    "deb": "gulp deb",
    "deploy-deb": "gulp deploy-deb",
    "build-docker-image": "gulp build-docker-image",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "config": {
    "mocha_reporter": "spec"
  },
  "jshintConfig": {
    "node": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "funcscope": true,
    "nocomma": true,
    "nonew": true,
    "strict": true,
    "undef": true,
    "unused": true,
    "validthis": true,
    "esversion": 6
  }
}
