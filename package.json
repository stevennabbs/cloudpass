{
  "name": "cloudpass",
  "version": "1.7.2",
  "description": "User management system",
  "keywords": [
    "user management"
  ],
  "author": {
    "email": "msuret@dhatim.com",
    "name": "Maxime Suret"
  },
  "homepage": "https://github.com/dhatim/cloudpass/issues",
  "bugs": {
    "url": "https://github.com/dhatim/cloudpass/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhatim/cloudpass.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.11.1",
    "npm": ">=3.8.2"
  },
  "main": "src/main.js",
  "dependencies": {
    "@fczbkk/url-match": "3.3.5",
    "auth-header": "1.0.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "cls-hooked": "4.2.2",
    "compression": "1.7.3",
    "config": "1.30.0",
    "cookie-parser": "1.4.3",
    "es6-error": "4.1.1",
    "express": "4.15.2",
    "health-checkup": "1.0.19",
    "js-yaml": "3.12.0",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.9",
    "moment": "2.22.2",
    "morgan": "1.9.0",
    "nocache": "2.0.0",
    "node-appinfo": "0.2.3",
    "nodemailer": "4.6.8",
    "nodemailer-mandrill-transport": "1.2.0",
    "optional-js": "2.0.0",
    "ovh-winston-ldp": "0.3.3",
    "passport": "0.4.0",
    "passport-http": "0.3.0",
    "passport-jwt-cookiecombo": "1.0.5",
    "pem": "1.12.5",
    "pg": "6.4.2",
    "pg-hstore": "2.3.2",
    "qr-image": "3.2.0",
    "randomstring": "1.1.5",
    "saml2-js": "2.0.3",
    "sequelize": "4.37.10",
    "shimmer": "1.1.1",
    "speakeasy": "2.0.0",
    "ssacl": "https://codeload.github.com/msuret/ssacl/tar.gz/1.0.0",
    "swagger-express-mw": "0.7.0",
    "throw": "1.1.0",
    "umzug": "2.1.0",
    "winston": "2.4.2",
    "winston-config": "0.5.1"
  },
  "devDependencies": {
    "bootprint": "1.0.2",
    "bootprint-openapi": "1.1.0",
    "coveralls": "3.0.1",
    "cross-conf-env": "1.1.2",
    "dockerode": "2.5.5",
    "dockerode-promise": "0.1.0",
    "env-test": "1.0.0",
    "gh-pages-travis": "1.0.4",
    "gulp": "3.9.1",
    "gulp-deb": "0.2.1",
    "gulp-rename": "1.4.0",
    "istanbul": "1.1.0-alpha.1",
    "jshint": "2.9.6",
    "merge-stream": "1.0.1",
    "mocha": "5.2.0",
    "rewire": "4.0.0",
    "rimraf": "2.6.2",
    "smtp-tester": "1.2.0",
    "sqlite3": "4.0.1",
    "superagent": "3.8.2",
    "supertest": "3.1.0",
    "tar-fs": "1.16.3",
    "thenify": "3.3.0"
  },
  "scripts": {
    "start": "node .",
    "clean": "rimraf build",
    "test:unit": "cross-conf-env node node_modules/istanbul/lib/cli.js cover --config=tests/unit/.istanbul.yml node_modules/mocha/bin/_mocha -- --opts tests/mocha.opts  --reporter $npm_package_config_mocha_reporter tests/unit",
    "test:integration": "cross-conf-env node node_modules/istanbul/lib/cli.js cover --config=tests/integration/.istanbul.yml node_modules/mocha/bin/_mocha -- --opts tests/mocha.opts  --reporter $npm_package_config_mocha_reporter tests/integration",
    "lint": "jshint src",
    "pretest": "npm run lint",
    "test": "cross-conf-env node node_modules/istanbul/lib/cli.js cover --config=tests/.istanbul.yml node_modules/mocha/bin/_mocha -- --opts tests/mocha.opts --reporter $npm_package_config_mocha_reporter tests",
    "coveralls": "cat build/reports/coverage/global/lcov.info | coveralls",
    "api-doc": "bootprint openapi swagger/swagger.yaml build/reports/api-doc",
    "gh-pages": "gh-pages-travis",
    "deb": "gulp deb",
    "deploy-deb": "gulp deploy-deb",
    "build-docker-image": "gulp build-docker-image",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "config": {
    "mocha_reporter": "spec"
  },
  "jshintConfig": {
    "node": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "funcscope": true,
    "nocomma": true,
    "nonew": true,
    "strict": true,
    "undef": true,
    "unused": true,
    "validthis": true,
    "esversion": 6
  }
}
