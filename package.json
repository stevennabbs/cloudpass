{
  "name": "cloudpass",
  "version": "1.6.1",
  "description": "User management system",
  "keywords": [
    "user management"
  ],
  "author": {
    "email": "msuret@dhatim.com",
    "name": "Maxime Suret"
  },
  "homepage": "https://github.com/dhatim/cloudpass/issues",
  "bugs": {
    "url": "https://github.com/dhatim/cloudpass/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhatim/cloudpass.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.9.5",
    "npm": ">=3.8.2"
  },
  "main": "src/main.js",
  "dependencies": {
    "@fczbkk/url-match": "3.3.3",
    "auth-header": "0.3.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "config": "1.29.0",
    "continuation-local-storage": "3.2.1",
    "cookie-parser": "1.4.3",
    "es6-error": "4.0.2",
    "express": "4.15.2",
    "health-checkup": "1.0.18",
    "js-yaml": "3.10.0",
    "jsonwebtoken": "8.1.0",
    "lodash": "4.17.4",
    "moment": "2.19.4",
    "morgan": "1.9.0",
    "nocache": "2.0.0",
    "node-appinfo": "0.2.3",
    "nodemailer": "4.4.1",
    "optional-js": "1.2.1",
    "passport": "0.4.0",
    "passport-http": "0.3.0",
    "passport-jwt-cookiecombo": "1.0.5",
    "pem": "1.12.3",
    "pg": "6.4.2",
    "pg-hstore": "2.3.2",
    "qr-image": "3.2.0",
    "randomstring": "1.1.5",
    "saml2-js": "2.0.1",
    "sequelize": "4.28.6",
    "shimmer": "1.1.1",
    "speakeasy": "2.0.0",
    "ssacl": "https://codeload.github.com/msuret/ssacl/tar.gz/1.0.0",
    "swagger-express-mw": "0.7.0",
    "throw": "1.1.0",
    "umzug": "2.1.0",
    "winston": "2.4.0",
    "winston-config": "0.5.1"
  },
  "devDependencies": {
    "bootprint": "1.0.0",
    "bootprint-openapi": "1.1.0",
    "coveralls": "3.0.0",
    "cross-conf-env": "1.1.2",
    "dockerode": "2.5.3",
    "dockerode-promise": "0.1.0",
    "env-test": "1.0.0",
    "gh-pages-travis": "1.0.4",
    "gulp": "3.9.1",
    "gulp-deb": "0.2.1",
    "gulp-rename": "1.2.2",
    "istanbul": "0.4.5",
    "jshint": "2.9.5",
    "merge-stream": "1.0.1",
    "mocha": "4.0.1",
    "rewire": "3.0.2",
    "rimraf": "2.6.2",
    "smtp-tester": "1.1.0",
    "sqlite3": "3.1.13",
    "superagent": "3.8.2",
    "supertest": "3.0.0",
    "tar-fs": "1.16.0",
    "thenify": "3.3.0"
  },
  "scripts": {
    "start": "node .",
    "clean": "rimraf build",
    "test:unit": "cross-conf-env node node_modules/istanbul/lib/cli.js cover --config=tests/unit/.istanbul.yml node_modules/mocha/bin/_mocha -- --opts tests/mocha.opts  --reporter $npm_package_config_mocha_reporter tests/unit",
    "test:integration": "cross-conf-env node node_modules/istanbul/lib/cli.js cover --config=tests/integration/.istanbul.yml node_modules/mocha/bin/_mocha -- --opts tests/mocha.opts  --reporter $npm_package_config_mocha_reporter tests/integration",
    "lint": "jshint src",
    "pretest": "npm run lint",
    "test": "cross-conf-env node node_modules/istanbul/lib/cli.js cover --config=tests/.istanbul.yml node_modules/mocha/bin/_mocha -- --opts tests/mocha.opts --reporter $npm_package_config_mocha_reporter tests",
    "coveralls": "cat build/reports/coverage/global/lcov.info | coveralls",
    "api-doc": "bootprint openapi swagger/swagger.yaml build/reports/api-doc",
    "gh-pages": "gh-pages-travis",
    "deb": "gulp deb",
    "deploy-deb": "gulp deploy-deb",
    "build-docker-image": "gulp build-docker-image",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "config": {
    "mocha_reporter": "spec"
  },
  "jshintConfig": {
    "node": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "funcscope": true,
    "nocomma": true,
    "nonew": true,
    "strict": true,
    "undef": true,
    "unused": true,
    "validthis": true,
    "esversion": 6
  }
}
